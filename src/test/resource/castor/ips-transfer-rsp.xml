<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ips.transfer.rsp.TransferRespXml">
    <map-to xml="transferRespXml"/>
    <field name="head" nillable="true" required="true" type="com.xpay.pay.proxy.ips.transfer.rsp.Head">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" nillable="true" required="true" type="com.xpay.pay.proxy.ips.transfer.rsp.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.transfer.rsp.Head">
    <map-to xml="head"/>
    <field name="version" nillable="true" required="true" type="string">
      <bind-xml name="version" node="element"/>
    </field>
    <field name="signature" nillable="true" required="true" type="string">
      <bind-xml name="signature" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.transfer.rsp.Body">
    <map-to xml="body"/>
    <field name="ipsBillNo" nillable="true" required="true" type="string">
      <bind-xml name="ipsBillNo" node="element"/>
    </field>
    <field name="tradeId" nillable="true" required="true" type="string">
      <bind-xml name="tradeId" node="element"/>
    </field>
    <field name="ipsFee" nillable="true" required="true" type="string">
      <bind-xml name="ipsFee" node="element"/>
    </field>
    <field name="tradeState" nillable="true" required="true" type="string">
      <bind-xml name="tradeState" node="element"/>
    </field>
  </class>

</mapping>