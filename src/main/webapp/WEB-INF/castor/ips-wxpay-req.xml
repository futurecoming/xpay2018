<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ips.wxpay.payreq.Ips">
    <map-to xml="Ips"/>
    <field name="wxPayReq" nillable="true" required="true" type="com.xpay.pay.proxy.ips.wxpay.payreq.WxPayReq">
      <bind-xml name="WxPayReq" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.wxpay.payreq.WxPayReq">
    <map-to xml="WxPayReq"/>
    <field name="head" nillable="true" required="true" type="com.xpay.pay.proxy.ips.common.ReqHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" nillable="true" required="true" type="com.xpay.pay.proxy.ips.wxpay.payreq.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.wxpay.payreq.Body">
    <map-to xml="body"/>
    <field name="merBillno" nillable="true" required="true" type="string">
      <bind-xml name="MerBillno" node="element"/>
    </field>
    <field name="goodsInfo" nillable="true" required="true" type="com.xpay.pay.proxy.ips.wxpay.payreq.GoodsInfo">
      <bind-xml name="GoodsInfo" node="element"/>
    </field>
    <field name="merType" nillable="true" required="true" type="string">
      <bind-xml name="MerType" node="element"/>
    </field>
    <field name="subMerCode" nillable="true" required="true" type="string">
      <bind-xml name="SubMerCode" node="element"/>
    </field>
    <field name="ordAmt" nillable="true" required="true" type="string">
      <bind-xml name="OrdAmt" node="element"/>
    </field>
    <field name="ordTime" nillable="true" required="true" type="string">
      <bind-xml name="OrdTime" node="element"/>
    </field>
    <field name="merchantUrl" nillable="true" required="true" type="string">
      <bind-xml name="MerchantUrl" node="element"/>
    </field>
    <field name="serverUrl" nillable="true" required="true" type="string">
      <bind-xml name="ServerUrl" node="element"/>
    </field>
    <field name="billEXP" nillable="true" required="true" type="string">
      <bind-xml name="BillEXP" node="element"/>
    </field>
    <field name="reachBy" nillable="true" required="true" type="string">
      <bind-xml name="ReachBy" node="element"/>
    </field>
    <field name="reachAddress" nillable="true" required="true" type="string">
      <bind-xml name="ReachAddress" node="element"/>
    </field>
    <field name="currencyType" nillable="true" required="true" type="string">
      <bind-xml name="CurrencyType" node="element"/>
    </field>
    <field name="attach" nillable="true" required="true" type="string">
      <bind-xml name="Attach" node="element"/>
    </field>
    <field name="retEncodeType" nillable="true" required="true" type="string">
      <bind-xml name="RetEncodeType" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.wxpay.payreq.GoodsInfo">
    <map-to xml="GoodsInfo"/>
    <field name="goodsName" nillable="true" required="true" type="string">
      <bind-xml name="GoodsName" node="element"/>
    </field>
    <field name="goodsCount" nillable="true" required="true" type="string">
      <bind-xml name="GoodsCount" node="element"/>
    </field>
  </class>
</mapping>