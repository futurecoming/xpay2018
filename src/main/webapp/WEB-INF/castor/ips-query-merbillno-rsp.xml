<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>

  <include href="ips-common.xml"/>

  <class name="com.xpay.pay.proxy.ips.query.merbillno.rsp.Ips">
    <map-to xml="Ips"/>
    <field name="orderQueryRsp" type="com.xpay.pay.proxy.ips.query.merbillno.rsp.OrderQueryRsp">
      <bind-xml name="OrderQueryRsp" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.query.merbillno.rsp.OrderQueryRsp">
    <map-to xml="OrderQueryReq"/>
    <field name="head" type="com.xpay.pay.proxy.ips.common.RspHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" type="com.xpay.pay.proxy.ips.query.merbillno.rsp.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.query.merbillno.rsp.Body">
    <map-to xml="body"/>
    <field name="merBillNo" type="string">
      <bind-xml name="MerBillNo" node="element"/>
    </field>
    <field name="ipsBillNo" type="string">
      <bind-xml name="IpsBillNo" node="element"/>
    </field>
    <field name="tradeType" type="string">
      <bind-xml name="TradeType" node="element"/>
    </field>
    <field name="currency" type="string">
      <bind-xml name="Currency" node="element"/>
    </field>
    <field name="amount" type="string">
      <bind-xml name="Amount" node="element"/>
    </field>
    <field name="merBillDate" type="string">
      <bind-xml name="MerBillDate" node="element"/>
    </field>
    <field name="ipsBillTime" type="string">
      <bind-xml name="IpsBillTime" node="element"/>
    </field>
    <field name="attach" type="string">
      <bind-xml name="Attach" node="element"/>
    </field>
    <field name="status" type="string">
      <bind-xml name="Status" node="element"/>
    </field>
  </class>

</mapping>