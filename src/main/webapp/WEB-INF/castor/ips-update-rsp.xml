<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ips.userupdate.rsp.UpdateUserRespXml">
    <map-to xml="updateUserRespXml"/>
    <field name="head" nillable="true" required="true" type="com.xpay.pay.proxy.ips.userupdate.rsp.Head">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" nillable="true" required="true" type="com.xpay.pay.proxy.ips.userupdate.rsp.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.userupdate.rsp.Head">
    <map-to xml="head"/>
    <field name="version" nillable="true" required="true" type="string">
      <bind-xml name="version" node="element"/>
    </field>
    <field name="signature" nillable="true" required="true" type="string">
      <bind-xml name="signature" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.userupdate.rsp.Body">
    <map-to xml="body"/>
    <field name="result" nillable="true" required="true" type="string">
      <bind-xml name="result" node="element"/>
    </field>
    <field name="customerCode" nillable="true" required="true" type="string">
      <bind-xml name="customerCode" node="element"/>
    </field>
    <field name="reason" nillable="true" required="true" type="string">
      <bind-xml name="reason" node="element"/>
    </field>
  </class>

</mapping>