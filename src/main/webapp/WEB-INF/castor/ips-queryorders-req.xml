<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
        "http://castor.org/mapping.dtd">

<mapping>

    <class name="com.xpay.pay.proxy.ips.queryorders.req.QueryOrdersReqXml">
        <map-to xml="queryOrdersReqXml"/>
        <field name="head" nillable="true" required="true" type="com.xpay.pay.proxy.ips.common.RequestHead">
            <bind-xml name="head" node="element"/>
        </field>
        <field name="body" nillable="true" required="true" type="com.xpay.pay.proxy.ips.queryorders.req.Body">
            <bind-xml name="body" node="element"/>
        </field>
    </class>

    <class name="com.xpay.pay.proxy.ips.queryorders.req.Body">
        <map-to xml="body"/>
        <field name="merAcctNo" nillable="true" required="true" type="string">
            <bind-xml name="merAcctNo" node="element"/>
        </field>
        <field name="customerCode" nillable="true" required="true" type="string">
            <bind-xml name="customerCode" node="element"/>
        </field>
        <field name="ordersType" nillable="true" required="true" type="string">
            <bind-xml name="ordersType" node="element"/>
        </field>
        <field name="merBillNo" nillable="true" required="true" type="string">
            <bind-xml name="merBillNo" node="element"/>
        </field>
        <field name="ipsBillNo" nillable="true" required="true" type="string">
            <bind-xml name="ipsBillNo" node="element"/>
        </field>
        <field name="startTime" nillable="true" required="true" type="string">
            <bind-xml name="startTime" node="element"/>
        </field>
        <field name="endTime" nillable="true" required="true" type="string">
            <bind-xml name="endTime" node="element"/>
        </field>
        <field name="currentPage" nillable="true" required="true" type="string">
            <bind-xml name="currentPage" node="element"/>
        </field>
        <field name="pageSize" nillable="true" required="true" type="string">
            <bind-xml name="pageSize" node="element"/>
        </field>
    </class>

</mapping>