<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ips.wxpay.notify.Ips">
    <map-to xml="Ips"/>
    <field name="wxPayRsp" nillable="true" required="true" type="com.xpay.pay.proxy.ips.wxpay.notify.WxPayRsp">
      <bind-xml name="WxPayRsp" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.wxpay.notify.WxPayRsp">
    <map-to xml="WxPayRsp"/>
    <field name="head" nillable="true" required="true" type="com.xpay.pay.proxy.ips.common.RspHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" nillable="true" required="true" type="com.xpay.pay.proxy.ips.wxpay.notify.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.wxpay.notify.Body">
    <map-to xml="body"/>
    <field name="merBillno" nillable="true" required="true" type="string">
      <bind-xml name="MerBillno" node="element"/>
    </field>
    <field name="merCode" nillable="true" required="true" type="string">
      <bind-xml name="MerCode" node="element"/>
    </field>
    <field name="account" nillable="true" required="true" type="string">
      <bind-xml name="Account" node="element"/>
    </field>
    <field name="ipsBillno" nillable="true" required="true" type="string">
      <bind-xml name="IpsBillno" node="element"/>
    </field>
    <field name="ipsBillTime" nillable="true" required="true" type="string">
      <bind-xml name="IpsBillTime" node="element"/>
    </field>
    <field name="ordAmt" nillable="true" required="true" type="string">
      <bind-xml name="OrdAmt" node="element"/>
    </field>
    <field name="retEncodeType" nillable="true" required="true" type="string">
      <bind-xml name="RetEncodeType" node="element"/>
    </field>
    <field name="status" nillable="true" required="true" type="string">
      <bind-xml name="Status" node="element"/>
    </field>
  </class>

</mapping>