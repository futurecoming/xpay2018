<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ips.refund.req.Ips">
    <map-to xml="Ips"/>
    <field name="refundReq" required="true" type="com.xpay.pay.proxy.ips.refund.req.RefundReq">
      <bind-xml name="RefundReq" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.refund.req.RefundReq">
    <map-to xml="RefundReq"/>
    <field name="head" required="true" type="com.xpay.pay.proxy.ips.common.ReqHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" required="true" type="com.xpay.pay.proxy.ips.refund.req.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.refund.req.Body">
    <map-to xml="body"/>
    <field name="orgMerTime" required="true" type="string">
      <bind-xml name="OrgMerTime" node="element"/>
    </field>
    <field name="orgMerBillNo" required="true" type="string">
      <bind-xml name="OrgMerBillNo" node="element"/>
    </field>
    <field name="billAmount" required="true" type="string">
      <bind-xml name="BillAmount" node="element"/>
    </field>
    <field name="refundAmount" required="true" type="string">
      <bind-xml name="RefundAmount" node="element"/>
    </field>
    <field name="refundMemo" required="true" type="string">
      <bind-xml name="RefundMemo" node="element"/>
    </field>
    <field name="merBillNo" required="true" type="string">
      <bind-xml name="MerBillNo" node="element"/>
    </field>
  </class>
</mapping>