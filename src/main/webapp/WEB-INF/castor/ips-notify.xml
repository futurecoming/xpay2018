<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ips.notify.Ips">
    <map-to xml="Ips"/>
    <field name="gateWayRsp" required="true" type="com.xpay.pay.proxy.ips.notify.GateWayRsp">
      <bind-xml name="GateWayRsp" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.notify.GateWayRsp">
    <map-to xml="GateWayRsp"/>
    <field name="head" required="true" type="com.xpay.pay.proxy.ips.common.RspHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" required="true" type="com.xpay.pay.proxy.ips.notify.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.notify.Body">
    <map-to xml="body"/>
    <field name="merBillNo" required="true" type="string">
      <bind-xml name="MerBillNo" node="element"/>
    </field>
    <field name="currencyType" required="true" type="string">
      <bind-xml name="CurrencyType" node="element"/>
    </field>
    <field name="amount" required="true" type="string">
      <bind-xml name="Amount" node="element"/>
    </field>
    <field name="date" required="true" type="string">
      <bind-xml name="Date" node="element"/>
    </field>
    <field name="status" required="true" type="string">
      <bind-xml name="Status" node="element"/>
    </field>
    <field name="msg" required="true" type="string">
      <bind-xml name="Msg" node="element"/>
    </field>
    <field name="attach" required="true" type="string">
      <bind-xml name="Attach" node="element"/>
    </field>
    <field name="ipsBillNo" required="true" type="string">
      <bind-xml name="IpsBillNo" node="element"/>
    </field>
    <field name="ipsTradeNo" required="true" type="string">
      <bind-xml name="IpsTradeNo" node="element"/>
    </field>
    <field name="retEncodeType" required="true" type="string">
      <bind-xml name="RetEncodeType" node="element"/>
    </field>
    <field name="bankBillNo" required="true" type="string">
      <bind-xml name="BankBillNo" node="element"/>
    </field>
    <field name="resultType" required="true" type="string">
      <bind-xml name="ResultType" node="element"/>
    </field>
    <field name="ipsBillTime" required="true" type="string">
      <bind-xml name="IpsBillTime" node="element"/>
    </field>
  </class>

</mapping>