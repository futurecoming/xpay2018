<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>


  <class name="com.xpay.pay.proxy.ips.transfer.req.TransferReqXml">
    <map-to xml="transferReqXml"/>
    <field name="head" nillable="true" required="true" type="com.xpay.pay.proxy.ips.common.RequestHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" nillable="true" required="true" type="com.xpay.pay.proxy.ips.transfer.req.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.transfer.req.Body">
    <map-to xml="body"/>
    <field name="merBillNo" nillable="true" required="true" type="string">
      <bind-xml name="merBillNo" node="element"/>
    </field>
    <field name="transferType" nillable="true" required="true" type="string">
      <bind-xml name="transferType" node="element"/>
    </field>
    <field name="merAcctNo" nillable="true" required="true" type="string">
      <bind-xml name="merAcctNo" node="element"/>
    </field>
    <field name="customerCode" nillable="true" required="true" type="string">
      <bind-xml name="customerCode" node="element"/>
    </field>
    <field name="transferAmount" nillable="true" required="true" type="string">
      <bind-xml name="transferAmount" node="element"/>
    </field>
    <field name="collectionItemName" nillable="true" required="true" type="string">
      <bind-xml name="collectionItemName" node="element"/>
    </field>
    <field name="remark" nillable="true" required="true" type="string">
      <bind-xml name="remark" node="element"/>
    </field>
  </class>

</mapping>