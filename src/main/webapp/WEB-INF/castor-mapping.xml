<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ips.gatewayreq.Ips">
    <map-to xml="Ips"/>
    <field name="gateWayReq" type="com.xpay.pay.proxy.ips.gatewayreq.GateWayReq">
      <bind-xml name="GateWayReq" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.gatewayreq.GateWayReq">
    <map-to xml="GateWayReq"/>
    <field name="head" type="com.xpay.pay.proxy.ips.common.ReqHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" type="com.xpay.pay.proxy.ips.gatewayreq.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.common.ReqHead">
    <map-to xml="head"/>
    <field name="version" type="string">
      <bind-xml name="Version" node="element"/>
    </field>
    <field name="merCode" type="string">
      <bind-xml name="MerCode" node="element"/>
    </field>
    <field name="account" type="string">
      <bind-xml name="Account" node="element"/>
    </field>
    <field name="reqDate" type="string">
      <bind-xml name="ReqDate" node="element"/>
    </field>
    <field name="signature" type="string">
      <bind-xml name="Signature" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.gatewayreq.Body">
    <map-to xml="body"/>
    <field name="merBillNo" type="string">
      <bind-xml name="MerBillNo" node="element"/>
    </field>
    <field name="gatewayType" type="string">
      <bind-xml name="GatewayType" node="element"/>
    </field>
    <field name="merType" type="string">
      <bind-xml name="MerType" node="element"/>
    </field>
    <field name="subMerCode" type="string">
      <bind-xml name="SubMerCode" node="element"/>
    </field>
    <field name="date" type="string">
      <bind-xml name="Date" node="element"/>
    </field>
    <field name="currencyType" type="string">
      <bind-xml name="CurrencyType" node="element"/>
    </field>
    <field name="amount" type="string">
      <bind-xml name="Amount" node="element"/>
    </field>
    <field name="lang" type="string">
      <bind-xml name="Lang" node="element"/>
    </field>
    <field name="attach" type="string">
      <bind-xml name="Attach" node="element"/>
    </field>
    <field name="retEncodeType" type="string">
      <bind-xml name="RetEncodeType" node="element"/>
    </field>
    <field name="serverUrl" type="string">
      <bind-xml name="ServerUrl" node="element"/>
    </field>
    <field name="billEXP" type="string">
      <bind-xml name="BillEXP" node="element"/>
    </field>
    <field name="goodsName" type="string">
      <bind-xml name="GoodsName" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.gatewayrsp.Ips">
    <map-to xml="Ips"/>
    <field name="gateWayReq" type="com.xpay.pay.proxy.ips.gatewayrsp.GateWayRsp">
      <bind-xml name="GateWayRsp" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.gatewayrsp.GateWayRsp">
    <map-to xml="GateWayRsp"/>
    <field name="head" type="com.xpay.pay.proxy.ips.common.RspHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" type="com.xpay.pay.proxy.ips.gatewayrsp.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.common.RspHead">
    <map-to xml="head"/>
    <field name="referenceID" type="string">
      <bind-xml name="ReferenceID" node="element"/>
    </field>
    <field name="rspCode" type="string">
      <bind-xml name="RspCode" node="element"/>
    </field>
    <field name="rspMsg" type="string">
      <bind-xml name="RspMsg" node="element"/>
    </field>
    <field name="reqDate" type="string">
      <bind-xml name="ReqDate" node="element"/>
    </field>
    <field name="rspDate" type="string">
      <bind-xml name="RspDate" node="element"/>
    </field>
    <field name="signature" type="string">
      <bind-xml name="Signature" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.gatewayrsp.Body">
    <map-to xml="body"/>
    <field name="qrCode" type="string">
      <bind-xml name="QrCode" node="element"/>
    </field>
  </class>



  <class name="com.xpay.pay.proxy.ips.notify.Ips">
    <map-to xml="Ips"/>
    <field name="gateWayRsp" type="com.xpay.pay.proxy.ips.notify.GateWayRsp">
      <bind-xml name="GateWayRsp" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.notify.GateWayRsp">
    <map-to xml="GateWayRsp"/>
    <field name="head" type="com.xpay.pay.proxy.ips.common.RspHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" type="com.xpay.pay.proxy.ips.notify.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.notify.Body">
    <map-to xml="body"/>
    <field name="merBillNo" type="string">
      <bind-xml name="MerBillNo" node="element"/>
    </field>
    <field name="currencyType" type="string">
      <bind-xml name="CurrencyType" node="element"/>
    </field>
    <field name="amount" type="string">
      <bind-xml name="Amount" node="element"/>
    </field>
    <field name="date" type="string">
      <bind-xml name="Date" node="element"/>
    </field>
    <field name="status" type="string">
      <bind-xml name="Status" node="element"/>
    </field>
    <field name="msg" type="string">
      <bind-xml name="Msg" node="element"/>
    </field>
    <field name="attach" type="string">
      <bind-xml name="Attach" node="element"/>
    </field>
    <field name="ipsBillNo" type="string">
      <bind-xml name="IpsBillNo" node="element"/>
    </field>
    <field name="ipsTradeNo" type="string">
      <bind-xml name="IpsTradeNo" node="element"/>
    </field>
    <field name="retEncodeType" type="string">
      <bind-xml name="RetEncodeType" node="element"/>
    </field>
    <field name="bankBillNo" type="string">
      <bind-xml name="BankBillNo" node="element"/>
    </field>
    <field name="resultType" type="string">
      <bind-xml name="ResultType" node="element"/>
    </field>
    <field name="ipsBillTime" type="string">
      <bind-xml name="IpsBillTime" node="element"/>
    </field>
  </class>
</mapping>