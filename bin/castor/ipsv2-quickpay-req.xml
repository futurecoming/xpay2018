<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ipsv2.acquiresms.Ips">
    <map-to xml="Ips"/>
    <field name="acquireSmsReq" nillable="true" required="true" type="com.xpay.pay.proxy.ipsv2.acquiresms.AcquireSmsReq">
      <bind-xml name="AcquireSmsReq" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ipsv2.acquiresms.AcquireSmsReq">
    <map-to xml="AcquireSmsReq"/>
    <field name="head" nillable="true" required="true" type="com.xpay.pay.proxy.ips.common.ReqHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" nillable="true" required="true" type="com.xpay.pay.proxy.ipsv2.common.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ipsv2.common.Body">
    <map-to xml="body"/>
    <field name="ipsSignNo" nillable="true" required="true" type="string">
      <bind-xml name="IpsSignNo" node="element"/>
    </field>
  </class>

</mapping>