<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ips.query.ips.req.Ips">
    <map-to xml="Ips"/>
    <field name="tradeQueryReq" nillable="true" required="true"
      type="com.xpay.pay.proxy.ips.query.ips.req.TradeQueryReq">
      <bind-xml name="TradeQueryReq" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.query.ips.req.TradeQueryReq">
    <map-to xml="TradeQueryReq"/>
    <field name="head" nillable="true" required="true" type="com.xpay.pay.proxy.ips.common.ReqHead">
      <bind-xml name="head" node="element"/>
    </field>
    <field name="body" nillable="true" required="true" type="com.xpay.pay.proxy.ips.query.ips.req.Body">
      <bind-xml name="body" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.query.ips.req.Body">
    <map-to xml="body"/>
    <field name="ipsTradeNo" nillable="true" required="true" type="string">
      <bind-xml name="IpsTradeNo" node="element"/>
    </field>
  </class>

</mapping>