<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.org/mapping.dtd">

<mapping>

  <class name="com.xpay.pay.proxy.ips.query.merbillno.rsp.Ips">
    <map-to xml="Ips"/>
