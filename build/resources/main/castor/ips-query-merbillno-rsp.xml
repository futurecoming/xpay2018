      type="com.xpay.pay.proxy.ips.query.merbillno.rsp.OrderQueryRsp">
      <bind-xml name="OrderQueryRsp" node="element"/>
    </field>
  </class>

  <class name="com.xpay.pay.proxy.ips.query.merbillno.rsp.OrderQueryRsp">
    <map-to xml="OrderQueryReq"/>
